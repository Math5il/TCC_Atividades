#include <stdio.h>
#include <string.h>

// Estados do AFD
typedef enum {
    L0, L1, L2
} Estado;

// Função para calcular a transição
Estado transicao(Estado estado, char entrada) {
    switch (estado) {
        case L0:
            if (entrada == 'a' || entrada == 'b') {
                return L1;
            }
            break;
        case L1:
            if (entrada == 'a' || entrada == 'b') {
                return L2;
            }
            break;
        case L2:
            if (entrada == 'a' || entrada == 'b') {
                return L2;
            }
            break;
    }
    return -1; // Retorna -1 se não houver transição válida
}

int main() {
    char entrada[100];
    scanf("%s", entrada);
    int i;
    Estado estado = L0;

    for (i = 0; i < strlen(entrada); i++) {
        estado = transicao(estado, entrada[i]);
        if (estado == -1) {
            printf("AFD invalido.\n");
            return 0;
        }
    }

    if (estado == L2) {
        printf("AFD valido.\n");
    } else {
        printf("AFD invalido.\n");
    }

    return 0;
}
